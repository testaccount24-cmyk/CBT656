/* REXX */
  vals='85c4c5c6c7c7d3d4d5d6d7a2'x
  d=xrange('00'x,'FF'x)
  shad =copies('#',18)||copies('#'copies('$',16)'#',16)copies('#',18)
  shad2=copies('#',18)||copies('#'copies('"',16)'#',16)copies('#',18)
  bot='c4'x||copies('a2'x,16)||'d4'x
  dyn='c5'x||copies('a2'x,16)||'d5'x
  Do a = 1 to 16
     dyn=dyn||'85'x||substr(d,1+(a-1)*16,16)||'85'x
  End
  dyn=dyn||bot
  dyn2 = dyn
  alpha='ABCDEFGHIJKLMNOPQRSTUVWXYZ'
  alpha=alpha||'abcdefghijklmnopqrstuvwxyz'
  alphage=alpha
  shad3=copies('$',52)
  dyn4= copies('s',80)
  shad4=copies('#',80)

  d='EsssssssNe EsPsN ee e e e ee FsLsO ee e e e ee DsGsM eDsssssssM'
  e=d
  s='########## $$$$$ ## $ $ $ ## $$$$$ ## $ $ $ ## $$$$$ ##########'
  t='########## """"" ## " " " ## """"" ## " " " ## """"" ##########'

  b='EsssssssNe klllj ee k C j ee k n j ee kmmmj eDsssssssM'
  c='EsssssssNe klllj ee k C j ee k n j ee kmmmj eDsssssssM'
  q='########## $$$$$ ## $ $ $ ## $ $ $ ## $$$$$ ##########'
  v='########## """"" ## " " " ## " " " ## """"" ##########'
  Do Until rc > 0
    Address ispexec 'DISPLAY PANEL(GEPNL)'
