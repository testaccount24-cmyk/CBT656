/* rexx */
a.0 = 3
a.1 = 'stem'
a.2 = a.1
a.3 = a.1
b = 'var'
call sub
exit

sub : procedure expose a. b
if datatype(a.0) = "NUM" then say 'arg a is stem- contains item(s)' a.0
else say 'arg a is var'
if datatype(b.0) = "NUM" then say 'arg a is stem- contains item(s)' b.0
else say 'arg b is var'
return
