)ATTR
% TYPE(TEXT)   INTENS(HIGH) SKIP(ON)
+ TYPE(TEXT)   INTENS(LOW)
_ TYPE(INPUT)  INTENS(HIGH) JUST(LEFT)  PAD(' ') CAPS(ON)
@ TYPE(TEXT  ) INTENS(LOW)  JUST(LEFT)  PAD(' ') CAPS(OFF) COLOR(YELLOW)
)BODY WINDOW(35,10)

     +TABLE SPACE MASK _M       +

   _G@Restrict only

   _C+INSTANC1 %(PROD      )
   _D+INSTANC2 %(TEST1     )
   _E+INSTANC3 %(TEST2     )
   _F+INSTANC4 %(TEST3     )

)PROC
&CNAME  = .CURSOR
)END
