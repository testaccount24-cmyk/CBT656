)ATTR
+ TYPE(TEXT)  INTENS(LOW)
_ TYPE(INPUT) INTENS(HIGH) JUST(LEFT) PAD(' ')
? TYPE(INPUT) INTENS(LOW ) JUST(LEFT) PAD(' ') COLOR(YELLOW)
)BODY  WINDOW(61,7)
 CMD >?ZCMD

 => Dataset  : _getline                                     +
 => DataClas : _dataclas               +
 => StorClas : _storclas               +
 => MgmtClas : _mgmtclas               +

)END
