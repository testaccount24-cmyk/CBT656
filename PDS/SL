/* REXX */
ADDRESS ISPEXEC
MRC=0
"ISREDIT MACRO "
"ISREDIT FIND 'ENVIRONMENT DIVISION' FIRST";MRC=MRC+RC
IF RC = 0 THEN
"ISREDIT LABEL .ZCSR = .ED 0"
"ISREDIT FIND 'DATA DIVISION' NEXT";MRC=MRC+RC
IF RC = 0 THEN
"ISREDIT LABEL .ZCSR = .DD 0"
"ISREDIT FIND 'PROCEDURE DIVISION' NEXT";MRC=MRC+RC
IF RC = 0 THEN
"ISREDIT LABEL .ZCSR = .PD 0"
"ISREDIT FIND 'FILE SECTION'  FIRST";MRC=MRC+RC
IF RC = 0 THEN
"ISREDIT LABEL .ZCSR = .FS 0"
"ISREDIT FIND 'WORKING-STORAGE SECTION' FIRST";MRC=MRC+RC
IF RC = 0 THEN
"ISREDIT LABEL .ZCSR = .WS 0"
"ISREDIT FIND 'LINKAGE SECTION' FIRST";MRC=MRC+RC
IF RC = 0 THEN
"ISREDIT LABEL .ZCSR = .LS 0"
IF MRC = 24 THEN DO
    ZEDSMSG = "NO LABEL SET"
    "SETMSG MSG(ISRZ001)"
END
ELSE DO
    ZEDSMSG = "LABEL(S) SET"
    "SETMSG MSG(ISRZ000)"
END
"ISREDIT LOCATE .ZFIRST"
EXIT
